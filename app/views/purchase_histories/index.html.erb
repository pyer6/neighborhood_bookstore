<ul>
  <% @purchase_histories.each do |purchase_history| %>
    <li class="mt-3">
      <div>購入者: <%= purchase_history.user_name %></div>
      <div>商品名: <%= purchase_history.product_name %></div>
      <div>購入数: <%= purchase_history.quantity %></div>
      <div>金額: ¥<%= purchase_history.price %></div>
    </li>
  <% end %>
</ul>
<div class="btn btn-success mt-3">ボタン</div>
